{
    "name": "jobmetric/laravel-metadata",
    "description": "This package is for the metadata of different Laravel projects.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.22",
        "jobmetric/laravel-event-system": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^8.36",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "JobMetric\\Metadata\\": "src/",
            "JobMetric\\Metadata\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Metadata\\MetadataServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-metadata/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-metadata"
    }
}
